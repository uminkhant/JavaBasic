<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" stylesheets="@../util/application.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jdc.mkt.controller.DbFormController">
   <children>
      <HBox>
         <children>
            <Label styleClass="txt-size" text="Create New Database Form">
               <HBox.margin>
                  <Insets bottom="15.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label text="User Name " />
            <TextField fx:id="tf_user" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="58.0">
         <children>
            <Label prefHeight="17.0" prefWidth="64.0" text="Password " />
            <TextField fx:id="tf_pass" />
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" spacing="3.0">
         <children>
            <Button mnemonicParsing="false" onAction="#enter" text="Enter" />
            <Button layoutX="180.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label fx:id="lb_message" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
